<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="#c3e5f5">

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton35"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:background="@null"
                android:layout_alignParentStart="true"
                android:src="@drawable/exit" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton36"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:background="@null"
                android:src="@drawable/stars" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton37"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@+id/imageButton36"
                android:layout_toEndOf="@+id/imageButton36"
                android:background="@null"
                android:src="@drawable/sound" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton38"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@+id/imageButton37"
                android:layout_toEndOf="@+id/imageButton37"
                android:background="@null"
                android:src="@drawable/help" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton39"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@+id/imageButton38"
                android:layout_toEndOf="@+id/imageButton38"
                android:background="@null"
                android:src="@drawable/home" />

            <ImageView
                android:layout_width="600"
                android:layout_height="400"
                android:id="@+id/imageView10"
                android:scaleType="fitXY"
                android:src="@drawable/pangungusap"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton40"
                android:background="@null"
                android:src="@drawable/angamingmumuntingbahay"
                android:layout_below="@+id/imageButton38"
                android:layout_alignRight="@+id/imageButton36"
                android:layout_alignEnd="@+id/imageButton36" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton41"
                android:layout_alignBottom="@+id/imageButton40"
                android:layout_toRightOf="@+id/imageButton40"
                android:background="@null"
                android:layout_toEndOf="@+id/imageButton40"
                android:src="@drawable/sagubat" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton42"
                android:src="@drawable/mekanikongsimangkanor"
                android:layout_below="@+id/imageButton40"
                android:background="@null"
                android:layout_alignRight="@+id/imageButton39"
                android:layout_alignEnd="@+id/imageButton39" />
</RelativeLayout>

