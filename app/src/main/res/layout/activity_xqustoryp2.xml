<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="lolz.flippino.xqustoryp3"
    android:background="#c3e5f5">
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@null"
        android:id="@+id/global_stars"
        android:src="@drawable/global_stars"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/global_sound" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:background="@null"
        android:id="@+id/global_sound"
        android:src="@drawable/global_sound"
        android:layout_alignTop="@+id/global_help"
        android:layout_toStartOf="@+id/global_help" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="90dp"
        android:background="@null"
        android:id="@+id/global_help"
        android:src="@drawable/global_help"
        android:layout_alignTop="@+id/global_home"
        android:layout_toStartOf="@+id/global_home" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:background="@null"
        android:id="@+id/global_home"
        android:src="@drawable/global_home"
        android:layout_alignBottom="@+id/global_stars"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@null"
        android:id="@+id/global_exit"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/global_exit" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/global_label"
        android:src="@drawable/global_label"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/global_leftarrow" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView44"
        android:src="@drawable/xqubg2"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView20"
        android:src="@drawable/istory1cloud"
        android:layout_below="@+id/global_stars"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@null"
        android:id="@+id/global_leftarrow"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/global_leftarrow" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@null"
        android:id="@+id/global_rightarrow"
        android:src="@drawable/global_rightarrow"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView31"
        android:src="@drawable/xqustory1bus"
        android:layout_alignTop="@+id/imageView33"
        android:layout_toEndOf="@+id/global_leftarrow"
        android:layout_marginTop="39dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView33"
        android:src="@drawable/xqustory3person"
        android:layout_above="@+id/global_rightarrow"
        android:layout_toStartOf="@+id/global_help"
        android:layout_marginBottom="48dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView34"
        android:src="@drawable/xqustory3fish"
        android:layout_alignBottom="@+id/imageView44"
        android:layout_alignEnd="@+id/imageView44" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Siya ay nakakita ng palaisdaan at mga puno. Tuwang tuwa siya sa mga ito."
        android:id="@+id/textView17"
        android:textSize="50dp"
        android:layout_alignBottom="@+id/imageView44"
        android:layout_alignEnd="@+id/imageView34"
        android:textAlignment="center" />

</RelativeLayout>
