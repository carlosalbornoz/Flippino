<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="#c3e5f5">


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView96"
        android:src="@drawable/xquenvironment"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton251"
        android:layout_marginEnd="278dp"
        android:src="@drawable/bus"
        android:background="@null"
        android:layout_alignTop="@+id/imageView96"
        android:layout_alignEnd="@+id/imageView96"
        android:layout_marginTop="200dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton252"
        android:background="@null"
        android:src="@drawable/global_leftarrow"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/imageView97"
        android:layout_marginBottom="66dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton253"
        android:background="@null"
        android:src="@drawable/global_rightarrow"
        android:layout_alignTop="@+id/imageButton252"
        android:layout_alignEnd="@+id/imageView96"
        android:layout_marginEnd="82dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton254"
        android:layout_alignParentTop="true"
        android:background="@null"
        android:layout_alignParentStart="true"
        android:src="@drawable/global_exit" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView97"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/imageButton251"
        android:layout_marginEnd="126dp"
        android:src="@drawable/global_lessonlabel" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton255"
        android:background="@null"
        android:src="@drawable/global_stars"
        android:layout_alignParentTop="true"
        android:layout_alignEnd="@+id/imageButton251" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton256"
        android:background="@null"
        android:src="@drawable/global_sound"
        android:layout_alignBottom="@+id/imageButton255"
        android:layout_toStartOf="@+id/imageButton257" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton257"
        android:background="@null"
        android:src="@drawable/global_help"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/imageButton258" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton258"
        android:background="@null"
        android:src="@drawable/global_home"
        android:layout_alignTop="@+id/imageButton256"
        android:layout_toStartOf="@+id/imageButton253" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Ang Field Trip ni Xander sa Quezon"
        android:id="@+id/textView20"
        android:layout_alignTop="@+id/imageButton253"
        android:layout_centerHorizontal="true"
        android:textSize="60dp" />
</RelativeLayout>
