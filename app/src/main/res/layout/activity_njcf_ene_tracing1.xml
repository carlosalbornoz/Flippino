<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:background="#c3e5f5"
    tools:context="lolz.flippino.njcf_ene_tracing1">

    <!-- The primary full-screen view. This can be replaced with whatever view
         is needed to present your content, e.g. VideoView, SurfaceView,
         TextureView, etc. -->

    <!-- This FrameLayout insets its children based on system windows using
         android:fitsSystemWindows. -->
    <FrameLayout android:layout_width="match_parent" android:layout_height="match_parent"
        android:fitsSystemWindows="true">

        <LinearLayout android:id="@+id/fullscreen_content_controls" style="?metaButtonBarStyle"
            android:layout_width="match_parent" android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:background="@color/black_overlay" android:orientation="horizontal"
            tools:ignore="UselessParent">

        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView22"
                android:layout_marginLeft="59dp"
                android:layout_marginStart="59dp"
                android:src="@drawable/njcf_tracing_ene"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView23"
                android:src="@drawable/njcf_tracing_activityboxes"
                android:layout_marginBottom="47dp"
                android:layout_alignParentBottom="true"
                android:layout_alignRight="@+id/imageView22"
                android:layout_alignEnd="@+id/imageView22" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView24"
                android:src="@drawable/njcf_tracing_ene_dotted"
                android:layout_alignTop="@+id/imageView22"
                android:layout_toRightOf="@+id/imageView23"
                android:layout_toEndOf="@+id/imageView23" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton67"
                android:background="@null"
                android:src="@drawable/njcf_tracing_pencil"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/imageView22"
                android:layout_toEndOf="@+id/imageView22"
                android:layout_marginLeft="65dp"
                android:layout_marginStart="65dp" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton68"
                android:src="@drawable/global_leftarrow"
                android:background="@null"
                android:layout_alignTop="@+id/imageButton69"
                android:layout_alignLeft="@+id/imageView22"
                android:layout_alignStart="@+id/imageView22" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton69"
                android:src="@drawable/global_rightarrow"
                android:layout_alignBottom="@+id/imageView23"
                android:layout_alignRight="@+id/imageView24"
                android:background="@null"
                android:layout_alignEnd="@+id/imageView24" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton70"
                android:src="@drawable/global_exit"
                android:layout_above="@+id/imageView22"
                android:layout_alignRight="@+id/imageButton68"
                android:background="@null"
                android:layout_alignEnd="@+id/imageButton68" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton71"
                android:layout_alignParentTop="true"
                android:layout_alignLeft="@+id/imageView23"
                android:layout_alignStart="@+id/imageView23"
                android:layout_marginLeft="59dp"
                android:layout_marginStart="59dp"
                android:background="@null"
                android:src="@drawable/global_lessonlabel" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton72"
                android:background="@null"
                android:src="@drawable/global_stars"
                android:layout_alignParentTop="true"
                android:layout_toLeftOf="@+id/imageButton73"
                android:layout_toStartOf="@+id/imageButton73" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton73"
                android:background="@null"
                android:src="@drawable/global_sound"
                android:layout_alignBottom="@+id/imageButton72"
                android:layout_toLeftOf="@+id/imageButton74"
                android:layout_toStartOf="@+id/imageButton74" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton74"
                android:background="@null"
                android:src="@drawable/global_help"
                android:layout_above="@+id/imageView24"
                android:layout_toLeftOf="@+id/imageButton75"
                android:layout_toStartOf="@+id/imageButton75" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton75"
                android:background="@null"
                android:src="@drawable/global_home"
                android:layout_alignParentTop="true"
                android:layout_alignLeft="@+id/imageButton69"
                android:layout_alignStart="@+id/imageButton69" />
        </RelativeLayout>
    </FrameLayout>

</FrameLayout>
