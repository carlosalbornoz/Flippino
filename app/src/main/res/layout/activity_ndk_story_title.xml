<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="#c3e5f5">


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView86"
        android:layout_marginTop="73dp"
        android:src="@drawable/birdswclouds"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView87"
        android:src="@drawable/ndkland"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView88"
        android:src="@drawable/ndkkubo"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView89"
        android:src="@drawable/ndkdurian"
        android:layout_alignBottom="@+id/imageView88"
        android:layout_alignEnd="@+id/imageView86" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton212"
        android:src="@drawable/global_exit"
        android:layout_alignParentTop="true"
        android:background="@null"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView90"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/imageView86"
        android:src="@drawable/global_lessonlabel" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton213"
        android:background="@null"
        android:src="@drawable/global_stars"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/imageButton214" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton214"
        android:background="@null"
        android:src="@drawable/global_sound"
        android:layout_alignBottom="@+id/imageButton213"
        android:layout_alignStart="@+id/imageView89" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton215"
        android:src="@drawable/global_help"
        android:background="@null"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/imageButton214" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton216"
        android:layout_alignParentTop="true"
        android:background="@null"
        android:layout_toEndOf="@+id/imageButton215"
        android:src="@drawable/global_home" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Niyog at Durian sa Kubo ni Aling Nena"
        android:id="@+id/textView16"
        android:textSize="50dp"
        android:layout_above="@+id/imageView89"
        android:layout_toEndOf="@+id/imageView90" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton237"
        android:layout_below="@+id/imageView88"
        android:layout_toEndOf="@+id/imageButton212"
        android:background="@null"
        android:src="@drawable/global_leftarrow" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton238"
        android:background="@null"
        android:src="@drawable/global_rightarrow"
        android:layout_alignTop="@+id/imageButton237"
        android:layout_toEndOf="@+id/imageView89" />
</RelativeLayout>